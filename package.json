{
  "name": "moshi",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "cross-env RCT_METRO_PORT=8081 node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "postinstall": "node scripts/install-hooks-support.js",
    "android": "cross-env RCT_METRO_PORT=8081 node node_modules/react-native/local-cli/cli.js run-android",
    "build-android": "cd android && ./gradlew assembleRelease",
    "build-android-clean": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "clean-android": "cd android && ./gradlew clean",
    "upload-android": "pushy uploadApk android/app/build/outputs/apk/release/app-release.apk",
    "publish-android": "pushy bundle --platform android",
    "publish-ios": "pushy bundle --platform ios"
  },
  "dependencies": {
    "@ant-design/react-native": "^3.1.3",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "mobx": "4",
    "mobx-react": "^5.4.3",
    "mobx-react-lite": "^1.0.1",
    "moment": "^2.24.0",
    "qs": "^6.6.0",
    "react": "16.8.2",
    "react-native": "0.58.5",
    "react-native-base64": "^0.0.2",
    "react-native-device-info": "^0.26.4",
    "react-native-elements": "^1.0.0",
    "react-native-fs": "^2.13.3",
    "react-native-image-crop-picker": "^0.22.0",
    "react-native-material-ui": "^1.30.1",
    "react-native-render-html": "^4.1.1",
    "react-native-router-flux": "^4.0.6",
    "react-native-storage": "^1.0.0-beta.1",
    "react-native-vector-icons": "^6.3.0",
    "react-native-webview": "^5.2.1",
    "rn-fetch-blob": "^0.10.15",
    "rxjs": "^6.4.0",
    "rxjs-hooks": "^0.3.3",
    "showdown": "^1.9.0",
    "socket.io-client": "1",
    "styled-components": "^4.1.3",
    "wolfy87-eventemitter": "^5.2.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.6",
    "@types/lodash": "^4.14.121",
    "@types/moment": "^2.13.0",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.8.4",
    "@types/react-native": "^0.57.37",
    "@types/react-native-material-ui": "^1.31.1",
    "@types/react-native-vector-icons": "^4.6.4",
    "@types/react-navigation": "2",
    "@types/react-test-renderer": "^16.8.1",
    "@types/showdown": "^1.9.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/styled-components": "^4.1.10",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "cross-env": "^5.2.0",
    "jest": "24.1.0",
    "metro-react-native-babel-preset": "0.52.0",
    "react-test-renderer": "16.8.2",
    "ts-jest": "^23.10.5",
    "typescript": "^3.3.3"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
}
